#include "stdafx.h"
#include "iostream"
using namespace std;

int main()
{
	setlocale(LC_ALL, "ru");
	int n,x=0,x1,x2,i=0,j=0;
	cout << "Введите размер массива: ";
	cin >> n;
	cout << "Заполните массив: ";
	int* m = new int[n];
	for (int i = 0;i < n;i++) {
		cin >> m[i];
	}
	for (;i < n;i++) {
		for (;j < n;j++) {
			if (m[i] == m[j] && i != j) {
				x1 = i;
				x2 = j;
			}
		}
		j = 0;
	}
	cout << ++x1 << " " << ++x2<< endl;
	delete[] m;
	return 0;
}
