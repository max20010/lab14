#include "stdafx.h"
#include "iostream"
using namespace std;

int main()
{
	setlocale(LC_ALL, "ru");
	int n;
	cout << "Введите размер массива A: ";
	cin >> n;
	cout << "Заполните массив: ";
	int* m = new int[n];
	for (int i = 0;i < n;i++) {
		cin >> m[i];
	}
	int min = m[1];
	for (int i = 1;i <= n-1;i+=2) {
		if (m[i] < min) {
			min = m[i];
		}
	}
	cout << min << endl;
	delete[] m;
	return 0;
}
